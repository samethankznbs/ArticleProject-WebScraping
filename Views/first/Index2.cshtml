            <div class="form-group">
                <input type="hidden" name="id" value=3 />

                    <input type="submit" value="Yeniden Eskiye Sırala (Yayınlanma Tarihi)" class="custom-button">
            </div>
        </form>
        <form asp-controller="first" asp-action="Index2" method="post" class="input-group">
            <div class="form-group"></div> <!-- Boş alan -->
            <div class="form-group">
                <input type="hidden" name="id" value=4 />

                    <input type="submit" value="Atıf Sayısına Göre Sırala Büyükten-Küçüğe" class="custom-button">
            </div>
        </form>
        <form asp-controller="first" asp-action="Index2" method="post" class="input-group">
            <div class="form-group"></div> <!-- Boş alan -->
            <div class="form-group">
                <input type="hidden" name="id" value=5 />


                    <input type="submit" value="Atıf Sayısına Göre Sırala Küçükten-Büyüğe" class="custom-button">
            </div>
        </form>
        <h3 class="tittle">Yayın Ara</h3>
        <form asp-controller="first" asp-action="Index2" method="post" class="input-group">
            <div class="form-group">

                    <input type="text" id="search_yayin_adi" name="search_yayin_adi" style="padding: 8px;" placeholder="Arama Yayın Adı" class="custom-input">
                    <input type="text" id="search_yazar_adi" name="search_yazar_adi" style="padding: 8px;" placeholder="Arama Yazar Adı" class="custom-input">
                    <input type="text" id="search_ozet" name="search_ozet" style="padding: 8px;" placeholder="Arama Özet" class="custom-input">
                    <input type="text" id="search_keywords" name="search_keywords" style="padding: 8px;" placeholder="Arama Anahtar Kelimeler" class="custom-input">
                    <input type="text" id="search_id" name="search_id" style="padding: 8px;" placeholder="Arama ID" class="custom-input">
                <input type="hidden" name="id" value=6 />

                
            </div>
            <div class="form-group">
                    <input type="submit" value="ARA" class="custom-button">
            </div>
        </form>
        <form asp-controller="first" asp-action="Index2" method="post" class="input-group">
            <div class="form-group">

               

                <input type="hidden" name="id" value=0 />
            </div>
            <div class="form-group">
                    <input type="submit" value="Filtre Sıfırla" class="custom-button">
            </div>
        </form>

		<div class="inner-sec">

			<div class="left-blog-info-w3layouts-agileits text-left">
				<div class="row">
					@foreach (var item in ViewBag.Articles)
					{
						<div class="col-lg-4 card">
							<a href="">
                                <img src="https://i.pinimg.com/474x/76/29/90/762990ffe65439c71b5bafd804b88a3c.jpg" class="card-img-top img-fluid" alt="" style="width: 300px; height: 300px;">
							</a>
							<div class="card-body">

								<h5 class="card-title">
									<a href="">@item.yayin_adi</a>
								</h5>
                                @if (!string.IsNullOrEmpty(item.ozet) && item.ozet.Length > 70)
                                {
                                    string truncatedOzet = item.ozet.Substring(0, 70);
                                    truncatedOzet = truncatedOzet.Substring(0, truncatedOzet.LastIndexOf(" "));
                                    <p class="card-text mb-3">@truncatedOzet... </p>
                                }
                                else if (!string.IsNullOrEmpty(item.ozet))
                                {
                                    <p class="card-text mb-3">@item.ozet</p>
                                }
                                    @{
                                        string path = @"C:/Users/samethan/Desktop/pdfler/" + item.yayin_adi + ".pdf";
                                    }
                                    <form asp-controller="first" asp-action="Index3" method="post" class="input-group">
                                        <div class="form-group"></div> <!-- Boş alan -->
                                        <div class="form-group">
                                            <input type="hidden" id="url" name="url" value="@item.url_adres">


                                            <input type="submit" value="Devamını Oku" class="custom-button">
                                        </div>
                                    </form>

                                    <form asp-controller="first" asp-action="ShowPdf" method="post" class="input-group">
                                        <div class="form-group"></div> <!-- Boş alan -->
                                        <div class="form-group">
                                            <input type="hidden" id="url" name="url" value="@item.download">


                                            <input type="submit" value="Pdf Aç" class="custom-button">
                                        </div>
                                    </form>
                               
                                   
                               


							</div>
						</div>
						<br />

					}

				</div>
				<!--//left-->
			</div>
		</div>
	</div>
    </header>
</section>
